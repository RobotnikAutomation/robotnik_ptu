<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                pan_tilt_port:
                    port_name: "/dev/ttyUSB0"
                    baud_rate: 57142
                    <!-- baud_rate: 9615 -->
                    min_motor_id: 1
                    max_motor_id: 5
                    <!-- update_rate: 20 -->
                    <!-- update_rate: 10 -->
                    update_rate: 5
        </rosparam>
    </node>
</launch>
